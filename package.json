    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.8",
        "@fortawesome/free-solid-svg-icons": "^5.5.0",
        "@fortawesome/vue-fontawesome": "^0.1.2",
        "bulma": "^0.7.2",
        "bulma-extensions": "^4.0.0",
        "bulmaswatch": "^0.7.2",
        "i": "^0.3.6",
        "npm": "^6.4.1",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "vue-bulma-datepicker": "^1.3.6",
        "vue2-dropzone": "^3.5.2",
        "vue2-editor": "^2.6.6"
    }
